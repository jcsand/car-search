  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.mjs",
    "analyze": "cross-env NODE_ENV=production webpack --profile --json --config webpack.config.mjs > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "start": "cross-env DEV_SERVER=true NODE_ENV=development webpack serve --config webpack.config.mjs",
    "test": "jest",
    "serve": "serve -n -s dist",
    "serve:with-build": "run-s build serve",
    "lint": "run-s -c lint:prettier lint:eslint lint:stylelint",
    "lint:fix": "run-s -c lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "lint:prettier:fix": "prettier --ignore-path .gitignore --write .",
    "lint:stylelint": "stylelint --ignore-path .gitignore '**/*.{css,html,tsx}'",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "githooks": "npx simple-git-hooks",
    "postinstall": "yarn githooks"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint"
  },
